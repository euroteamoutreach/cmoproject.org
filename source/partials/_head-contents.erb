<meta charset="utf-8">
<meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="description" content=<%= page_description %> />
<meta name="robots" content="index,follow" />
<meta name="author" content="Joshua Steele" />

<title><%= full_title(current_page.data.title) %></title>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
<link href="http://fonts.googleapis.com/css?family=Oxygen:400,300,700" rel="stylesheet" type="text/css">

<%= tag "meta", property: "og:title", content: data.site.title %>
<%= tag "meta", property: "og:url", content: data.site.url %>
<%= tag "meta", property: "og:image", content: image_url("cmo-logo-full.png") %>

<%= tag "link", rel: "apple-touch-icon", href: "images/cmo-apple-touch-icon.png" %>

<%= favicon_tag "images/cmo-favicon.png" %>

<%= stylesheet_link_tag "all" %>
<%= javascript_include_tag "all" %>
